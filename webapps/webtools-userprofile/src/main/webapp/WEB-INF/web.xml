<web-app metadata-complete="true" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<context-param>
		<param-name>configuration</param-name>
		<param-value>deployment</param-value>
	</context-param>
	<context-param>
		<param-name>STAGE</param-name>
		<param-value>development</param-value>
	</context-param>
	<listener>
		<listener-class>com.thorstenmarx.webtools.userprofile.ContextListener</listener-class>
	</listener>
   
	<servlet>
		<servlet-name>UserProfileServlet</servlet-name>
		<servlet-class>com.thorstenmarx.webtools.userprofile.web.servlets.UserProfileServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserProfileServlet</servlet-name>
		<url-pattern>/up.js</url-pattern>
	</servlet-mapping>
	<absolute-ordering/>
</web-app>
